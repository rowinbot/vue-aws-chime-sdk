{
  "name": "@rowinbot/vue-aws-chime-sdk",
  "version": "0.0.1",
  "description": "Vue components and utilities for running realtime meetings with AWS Chime SDK",
  "author": "Rowin Hernandez - @rowinbot",
  "private": false,
  "packageManager": "pnpm@10.11.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rowinbot/vue-aws-chime-sdk.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "git@github.com:rowinbot/vue-aws-chime-sdk.git"
  },
  "homepage": "https://github.com/rowinbot/vue-aws-chime-sdk/packages/vue-aws-chime-sdk",
